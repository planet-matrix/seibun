{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"]
    },
    "dev": {
      "dependsOn": ["^dev"]
    },
    "dev:css": {
      "dependsOn": ["^dev:css"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
