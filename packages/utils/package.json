{
  "name": "@planet-matrix/utils",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/planet-matrix/seibun",
    "directory": "packages/utils"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "require": "./dist/hooks/index.cjs",
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts src/hooks/index.ts --format cjs,esm --dts",
    "build:clean": "rm -rf dist && pnpm run build",
    "dev": "pnpm run build -- --watch src"
  },
  "dependencies": {},
  "devDependencies": {
    "@planet-matrix/tsconfig": "^0.1.1",
    "@types/react": "^18.2.28"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
