{
  "name": "@hyoban/components",
  "version": "5.0.4",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "tailwindcss -i ./src/index.css -o ./src/styles.css",
    "build": "tsup src/index.tsx",
    "watch": "npm run build -- --watch src",
    "watch:css": "tailwindcss -i ./src/index.css -o ./src/styles.css --watch"
  },
  "dependencies": {
    "@hyoban/utils": "workspace:*",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@hyoban/tailwind-config": "workspace:^",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
