{
  "name": "@hyoban/components",
  "version": "7.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyoban/config",
    "directory": "packages/components"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./button": {
      "require": "./dist/button/index.cjs",
      "import": "./dist/button/index.js",
      "types": "./dist/button/index.d.ts"
    },
    "./checkbox": {
      "require": "./dist/checkbox/index.cjs",
      "import": "./dist/checkbox/index.js",
      "types": "./dist/checkbox/index.d.ts"
    },
    "./input": {
      "require": "./dist/input/index.cjs",
      "import": "./dist/input/index.js",
      "types": "./dist/input/index.d.ts"
    },
    "./label": {
      "require": "./dist/label/index.cjs",
      "import": "./dist/label/index.js",
      "types": "./dist/label/index.d.ts"
    },
    "./separator": {
      "require": "./dist/separator/index.cjs",
      "import": "./dist/separator/index.js",
      "types": "./dist/separator/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch src"
  },
  "dependencies": {
    "@hyoban/utils": "workspace:*",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@hyoban/tailwind-config": "workspace:^",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3"
  },
  "peerDependencies": {
    "@hyoban/components": "workspace:*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
