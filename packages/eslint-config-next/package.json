{
  "name": "@hyoban/eslint-config-next",
  "version": "4.0.5",
  "description": "hyoban's eslint config for Next.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@hyoban/eslint-config-react": "workspace:*",
    "@next/eslint-plugin-next": "^13.4.16"
  },
  "peerDependencies": {
    "eslint": ">= 8",
    "tailwindcss": ">= 3",
    "typescript": ">= 5"
  }
}
